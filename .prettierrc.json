{
  "plugins": ["@trivago/prettier-plugin-sort-imports", "prettier-plugin-sql"],

  "singleQuote": true,

  "importOrder": ["<THIRD_PARTY_MODULES>", "^[./]"],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,

  "language": "sql",
  "keywordCase": "lower",
  "dataTypeCase": "lower",
  "functionCase": "lower",
  "identifierCase": "lower",
  "newlineBeforeSemicolon": true
}
